setwd("C:/Users/hom42612/Desktop/Coursera/Exploratory Data Analysis/ExData_Plotting1")
#Load libraries
library(lubridate)
fullset <- read.table("household_power_consumption.txt",
header = TRUE, sep = ";",
nrows = 70000,
stringsAsFactors = FALSE)
#Convert Date field from character to date format
fullset$Date <- as.Date(fullset$Date, "%d/%m/%Y")
fullset$Time <- as.POSIXct(paste(fullset$Date, fullset$Time, sep = " "))
#Subset dataset to show data only for February 1 and 2 of 2007
febset <- subset(fullset, subset = (Date <= "2007-02-02" & Date >= "2007-02-01"))
febset$Sub_metering_1 <- as.numeric(febset$Sub_metering_1)
febset$Sub_metering_2 <- as.numeric(febset$Sub_metering_2)
febset$Sub_metering_3 <- as.numeric(febset$Sub_metering_3)
febset$Global_reactive_power <- as.numeric(febset$Global_reactive_power)
#Set parameters to include 4 charts on one page
png("Plot4.png", units = "px", width = 480, height = 480)
par(mfrow = c(2,2))
#Chart #1
plot(febset$Time, febset$Global_active_power, type="l",
xlab = "", ylab = "Global Active Power (kilowatts)")
#Chart #2
plot(febset$Time, febset$Voltage, type ="l",
xlab = "datetime", ylab = "Voltage")
#Chart #3
plot(febset$Time, febset$Sub_metering_1, type="n", xlab = "", ylab = "Energy sub metering")
lines(febset$Time, febset$Sub_metering_1, col = "black")
lines(febset$Time, febset$Sub_metering_2, col = "red")
lines(febset$Time, febset$Sub_metering_3, col = "blue")
legend("topright", lwd = 1, col = c("black", "red", "blue"),
legend = c("Sub_metering_1", "Sub_metering_2", "Sub_metering_3"))
#Chart #4
plot(febset$Time, febset$Global_reactive_power, type="l",
xlab = "datetime", ylab = "Global reactive power")
dev.off()
